<?xml version="1.0"?>
<configuration>
  <appSettings>    
    <add key="FirstStringValue" value="abc"></add>
    <add key="SecondStringValue" value="second string value"></add>    
    
    <add key="IntValue" value="1"></add>
    <add key="SecondIntValue" value="100"></add>
    
    <add key="EmptyIntValue" value=""></add>

    <add key="DoubleValue" value="1.1"></add>
    
    <!-- The value uses comman instead of period. -->
    <add key="DoubleWithFinnishLocale" value="1,1"/>

    <add key="EnumString" value="Started"></add>
    
    <add key="EnumNumeric" value="2"></add>
    
  </appSettings>

  <connectionStrings>
    <!-- 
    We need to clear other connection strings which are inherited from machine.config
    in order to use the ConnectionString property.
    -->
    <clear/>
    <add name="MyDatabase" connectionString="Data Source=localhost;Initial Catalog=MyDb;User Id=username;Password=password;"/>
  </connectionStrings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
